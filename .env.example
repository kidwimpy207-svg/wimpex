PORT=3001
NODE_ENV=development
API_PREFIX=/api

JWT_SECRET=REPLACE_WITH_SECRET
SESSION_SECRET=REPLACE_WITH_SECRET
JWT_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

DB_HOST=your-db-host
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=REPLACE_WITH_DB_PASSWORD

REDIS_REST_URL=
REDIS_TOKEN=REPLACE_WITH_REDIS_TOKEN

AWS_ACCESS_KEY_ID=REPLACE_WITH_AWS_KEY
AWS_SECRET_ACCESS_KEY=REPLACE_WITH_AWS_SECRET
